pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Build the job'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploy war file'
            }
        }
         stage('Test') {
            steps {
                echo 'Test Script'
            }
        }
         stage('Release') {
            steps {
                echo 'Release'
            }
        }
    }
}
   
